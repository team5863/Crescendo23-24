package frc.robot.commands;

import frc.robot.subsystems.Test;

import java.util.function.DoubleSupplier;

//import edu.wpi.first.wpilibj.DigitalInput;

//import java.util.function.DoubleSupplier;

import edu.wpi.first.wpilibj2.command.Command;

public class TestMotorRun extends Command {
    private final Test m_TestMotor;

    Double testSpeed;
    
    public TestMotorRun(Test m_TestMotor, Double testSpeed){    
        this.m_TestMotor = m_TestMotor;
        addRequirements(m_TestMotor);

        this.testSpeed = testSpeed;
    }

    // Called every time the scheduler runs while the command is scheduled.
    @Override
    public void execute() {
            m_TestMotor.testRun(testSpeed);

        }

    @Override
    public void end(boolean interrupted) {
            m_TestMotor.testRun(0);
        }
    }

